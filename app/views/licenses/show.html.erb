<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li class="first"><%= link_to 'List', licenses_path  %></li>
      <li><%= link_to 'New', new_license_path %></li>
      <li class="active"><%= link_to 'Show', license_path %></li>
    </ul>
    <div class="clear"></div>
  </div>          
  <div class="content">
    <div class="inner">
      
        <p>
          <b>Software:</b>
          <%= @license.software %>
        </p>

        <p>
          <b>Version:</b>
          <%= @license.version %>
        </p>

        <p>
          <b>Language:</b>
          <%= @license.language %>
        </p>

        <p>
          <b>Serial:</b>
          <%= @license.serial %>
        </p>

        <p>
          <b>User:</b>
          <%= @license.user %>
        </p>

        <p>
          <b>Os:</b>
          <%= @license.os %>
        </p>

        <p>
          <b>Licensetype:</b>
          <%= @license.licenseType %>
        </p>

        <p>
          <b>Numlicense:</b>
          <%= @license.numLicense %>
        </p>

        <p>
          <b>Numinstalled:</b>
          <%= @license.numInstalled %>
        </p>

        <p>
          <b>Hardware_id:</b>
          <%= @license.hardware_id %>
        </p>

        <p>
          <b>Server_id:</b>
          <%= @license.server_id %>
        </p>
      <%= link_to 'Edit', edit_license_path(@license) %>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>