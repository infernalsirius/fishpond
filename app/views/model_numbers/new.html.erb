    <ul>
      <li class="active"><%= link_to 'Nouveau', new_model_number_path %></li>
    </ul>
    <div class="clear"></div>
  </div>          
  <div class="content">            
    <h2 class="title">Nouveau Numéro de modèle</h2>
    <div class="inner">
      <% form_for :model_number, :url => model_numbers_path, :html => { :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>